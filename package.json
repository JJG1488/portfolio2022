    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "gh-pages": "^3.2.3",
    "mongoose": "^6.1.4",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-router-bootstrap": "^0.26.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "web-vitals": "^2.1.2"
